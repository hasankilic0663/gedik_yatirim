import Foundation
import SwiftUI

struct gedik_text: ViewModifier {
    func body(content : Content) -> some View {
        content
            .font(
            Font.custom("Plus Jakarta Sans", size: 18)
            .weight(.medium)
            )
            .multilineTextAlignment(.center)
            .foregroundColor(Constants.ShadesWhite)
            .frame(width: 345, alignment: .top)
    }
}


